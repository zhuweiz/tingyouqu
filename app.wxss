/*  底部悬浮导航  */
.fix_nav_wp{position: fixed;left:0;bottom:0;background:#fff;border-top:1rpx solid #e5e5e5;z-index: 10}
.fix_nav_wp .nav_link{flex:1;}
.fix_nav_wp button{height:110rpx;display: flex;justify-content:space-between;align-items: center;padding:0;font-size:22rpx;
flex-direction: column;color:#999;position: relative;}
.fix_nav_wp .tip_tip{position: absolute;right:25rpx;top:5rpx;min-width:28rpx;line-height:32rpx;height: 32rpx;padding:0 8rpx;color:#fff;background:#f00;border-radius:50em;border:1rpx solid #fff;}
.fix_nav_wp .nav_link .iconfont{height:40rpx;font-size:56rpx;line-height: 80rpx;color: #c4c8cc;}
.fix_nav_wp .nav_link .plus_wp{width:120rpx;height:120rpx;margin-top: -23rpx;}
.fix_nav_wp .nav_link  image{width:40rpx;height:40rpx;}
.fix_nav_wp .nav_link .txt{height:50rpx;line-height:40rpx;color: #c4c8cc;}
.fix_nav_wp .nav_link.current .txt{color:#00a2e9;}
.fix_nav_wp .nav_link .del_ico{display: block;}
.fix_nav_wp .nav_link .cur_ico{display: none;}
.fix_nav_wp .nav_link.current .del_ico{display: none;}
.fix_nav_wp .nav_link.current .cur_ico{display: block;color:#00a2e9;}
.fix_nav_wp .nav_link .txt_fb{line-height: 18rpx}
.fix_nav_wp .nav_link .mp_ico{font-size: 50rpx;line-height: 80rpx}
.fix_nav_wp .nav_link .xiaox_ico{font-size: 64rpx;line-height: 70rpx;}
.fix_nav_wp .nav_link .my_ico{font-size: 56rpx;}
.fix_nav_wp .on_cor .del_ico, .fix_nav_wp .on_cor .txt{color: #0D5EFF}
/* 清楚按钮的默认样式  */
.defalut_btn{background: transparent;border:none;overflow: visible;padding-left:0;padding-right:0;margin-left:0;margin-right:0;
border-radius:0;}
.defalut_btn:after{display: none;}
.text-primary{color:#00a2e9;}
.imggg{
  margin-top: 21rpx;
}
.left{
  float: left;
}
.right{
  float: right;
}
/**隐藏滑块**/
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
/* 提示通知 */
.notice-box{
  width: 100%;
  height: 125rpx;
  clear: both;
  margin-bottom: 10rpx;
  background: rgba(250, 240, 240, 0.8);
  /* box-shadow: 1px 1px 1px rgba(34, 25, 25, 0.2); */
}
.swiper_container {
  height: 90rpx;
  width: 80%;
  line-height:90rpx;
  text-align:center;
  color: red;
  margin-top: 20rpx;
  background: #fff;
}
.notice-box .ico{
  width: 10%;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  margin-top: 20rpx;
  background: #fff;
}
.notice-box .jiantou{
  height: 90rpx;
  width: 10%;
  text-align: center;
  line-height: 90rpx;
  margin-top: 20rpx;
  background: #fff;
}
.swiper_item2 {
  font-size: 25rpx;
  color: #000;
  overflow: hidden;  
  text-align: left;
  text-overflow: ellipsis;
  white-space: nowrap;
}
/* 广告 */
.ad-box{ 
  height: 310rpx; 
  margin-top: 35rpx;
  background-color: rgba(250, 240, 240, 0.8);
}
.ad-box image{ 
  height: 260rpx; 
  width: 92%; 
  margin: 0 4%;
  margin-top: 30rpx;
  border-radius: 10rpx;
  background-color: rgba(250, 240, 240, 0.8);
  box-shadow: 1px 1px 1px rgba(34, 25, 25, 0.4);
}



.goods-con{  
  height: auto;
  background-color: #fff;
}
.good-title{
  height: 100rpx;
  line-height: 120rpx;
  font-size: 38rpx;
  margin-left: 4%;
  font-weight: 800;
  color:#2EB3FF;
  letter-spacing: 4rpx;
}
.goods-box{ 
  height: auto;
  width: 92%;
  margin: 0 4%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding-bottom: 40rpx;
}
.goods-item{
  width: 47%;
  height: 520rpx;
  background-color: #fff;
  margin-top: 25rpx;
  border-radius: 10rpx;
  border: 0.5px solid rgba(250, 240, 240, 1);
  box-shadow: 1px 1px 1px rgba(250, 240, 240, 1);
}
.goods-box .goods-item:nth-child(2n){  
  margin-left: 4%;
}

.goods-item .goods-img image{ 
  width: 100%;
  height: 360rpx;
  border-radius: 10rpx 10rpx 0 0;
}
.goods-item .goods-title {
  width: 250rpx;
  height: 50rpx;
}
.goods-item .goods-title text{
  width: 250rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: left;
  margin-left: 10rpx;
  font-size: 30rpx;
  font-weight: 800;
  color: #333;
  display: block;
  text-overflow:ellipsis;white-space:nowrap;overflow:hidden;
}
.goods-item .goods-price text{
  height: 50rpx;
  line-height: 50rpx;
  color: red;
}
.goods-item .goods-price text:nth-child(2){  
  margin-left: 10rpx;
  font-size: 30rpx;
}
.goods-item .goods-price text:nth-child(3){  
  margin-left: 5rpx;
  font-size: 24rpx;
}
.goods-item .goods-price image{
  height: 30rpx;
  width: 30rpx;
  line-height: 30rpx;
  border-radius: 30rpx;
  margin-left: 10rpx;
  position: relative;
  bottom: -3rpx;
}
.goods-item .user-info image{
  height: 35rpx;
  width: 35rpx;
  line-height: 35rpx;
  border-radius: 35rpx;
  margin-top: 10rpx;
  margin-left: 10rpx;
}

.goods-item .user-info .nickname{
  height: 40rpx;
  width: 100rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  margin-left: 10rpx;
  color: #333;text-overflow:ellipsis;white-space:nowrap;overflow:hidden;
}
.goods-item .user-info .addr{
  height: 40rpx;
  width: 100rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  margin-left: 10rpx;text-overflow:ellipsis;white-space:nowrap;overflow:hidden;
  /* border: 1px solid rebeccapurple; */
} 


/*招领*/
.recruit-con{  
  height: auto; 
}
.recruit-box{ 
  height: auto;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-bottom: 40rpx;
}
.recruit-item{
  width: 100%;
  height: auto;
  margin-top: 25rpx;
  background-color: #fff; 
  /* box-shadow: 1px 1px 1px rgba(34, 25, 25, 0.2); */
  padding-bottom: 20rpx;
}
.recruit-box .recruit-item:first-child{  
  margin-top: 0rpx;
}
.recruit-box .recruit-item:last-child{  
  margin-bottom: 90rpx;
}
.recruit-item .recruit-info{
  height: auto;
  width: 92%;
  margin: 0rpx 4%;
}
.recruit-info .top{  
  width: 100%;
  height: 120rpx;
  margin-top: 10rpx;
}
.recruit-info .top .user-img{
  width: 16%;
  height: 120rpx;
}
.recruit-info .top .user-img image{
  width: 90rpx;
  height: 90rpx;
  margin-top: 13rpx;
  border-radius: 90rpx;
  border: 2px solid #2EB3FF;
}
.recruit-info .top .user-name{
  width: 60%;
  height: 90rpx;  
  margin-top: 30rpx;
}
.recruit-info .top .user-name text{
  display: block;
  height: 45rpx;
  font-size: 30rpx;
  margin-left: 10rpx;
}
.recruit-info .top .user-name text:first-child{
  font-weight: 800;
  line-height: 30rpx;
}
.recruit-info .top .user-name text:last-child{
  line-height: 30rpx;
}
.recruit-info .top .contact{
  width: 18%;
  height: 120rpx;
  margin-left: 6%
}

.recruit-info .top .contact text{
  height: 120rpx;
  line-height: 120rpx;
  padding: 8rpx 20rpx;
  font-size: 25rpx;
  border: 1rpx solid #2EB3FF;
  border-radius: 10rpx;
}


.recruit-info .center{ 
  clear: both; 
  width: 100%;
  height: auto;
  font-size: 34rpx;
}
.recruit-info .center .tit{ 
  clear: both; 
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  color: #2EB3FF;
}

.recruit-info .center .content{ 
  clear: both; 
  width: 100%;
  line-height: 50rpx;
  color: #000;
}
.recruit-info .center .content .img-list image{ 
  width: 32%;
  height: 220rpx;
  margin-left: 2%;
  padding: 20rpx 0 0 0;
}
.recruit-info .center .content .img-list image:first-child{ 
  margin-left: 0%;
}
.recruit-info .center .address text{ 
  clear: both; 
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  color: #ccc;
  font-size: 32rpx;
}
.bottom{
  height: 80rpx;
  width: 96%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin: 0 2%;
}
.bottom-item{
  width: 25%;
  height: 80rpx;
  text-align: center;
  margin-top: 10rpx;
}
.bottom .bottom-item:first-child{
  text-align: left;
}
.bottom-item text{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 10rpx;
}
.bottom-item ico{
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
}