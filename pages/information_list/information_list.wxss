/* 头部搜索 */
.head {
  position: relative;
  width: 100%;
  height: 90rpx;
  font-size: 28rpx;
  z-index: 999;
  background: #fff;
}
.on{
   color: #0D5EFF;
}
.head-c{
    background: #fff;
    z-index: 999;
    position: fixed;
  width: 100%;
  height: 90rpx;
  background-position: 10rpx 10rpx;
  background-size: 100% 100%;
  /* border-bottom: 1rpx solid #DEE3F0; */
}

.nav {
  position: fixed;
  height: 72rpx;
  left: 5%;
  margin: auto;
  width: 690rpx;
  background: #F4F7FF;
  border-radius: 16rpx;
  /* margin-top: 25rpx; */
}

.icon {
  position: absolute;
  width: 20%;
  display: inline-block;
  height: 100%;
}

.icon image {
  position: absolute;
  top: 25rpx;
  left: 20rpx;
}

.nav_seek {
  position: absolute;
  width: 80%;
  left: 67rpx;
  opacity: 1;
  height: 100%;
  display: inline-block;
}
