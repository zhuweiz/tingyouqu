}
.fr{
  float: right;
}
.aaa{
  height: 239rpx;
  border: 1rpx solid #0D5EFF;
}
.bottomd{
  width: 100%;
  height: 220rpx;
  background: #fff;
}
.h1{
   padding-top: 20rpx;
    width: 650rpx;
  height: 140rpx;
  margin: auto;
}
.img image{
  width: 240rpx;
  height: 155rpx;
}
.img2{
  
  width: 350rpx;
