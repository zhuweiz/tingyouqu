.body{
  position: relative;
  margin-top: 30rpx;
  height: 237rpx;
  background: #fff;
}
.body_a{
   margin-left: 50rpx;
  height: 161rpx;
  border-bottom: 1rpx solid #DDE3F1;
}
.body_img{
  position: absolute;
  top: 0;
  right: 0;
  width: 50rpx;
  height: 50rpx;
}
.piao1{
float: left;
}
.piao1 view:nth-child(1){
  font-size:28rpx;
font-weight:600;
margin-top: 35rpx;
}
.piao1 view:nth-child(2){
  font-size:32rpx;
font-weight:500;
margin-top: 15rpx;
color:rgba(255,31,35,1);
}
.piao2{
  float: right;
  margin-right: 50rpx;
}
.body_b{
  margin-top: 22rpx;
  margin-left: 50rpx;
  font-size:24rpx;
font-family:PingFang SC;
font-weight:400;
  overflow: hidden;
width: 600rpx;
text-overflow: ellipsis;
white-space: nowrap;
}
.province{
  overflow: hidden;
width: 26rpx;
text-overflow: ellipsis;
white-space: nowrap;
}
.city_a{
  overflow: hidden;
width: 26rpx;
text-overflow: ellipsis;
white-space: nowrap;
margin-left: 5rpx;
}
.inventory{
  z-index: 99;
   overflow: hidden;
  display: inline-block;
  width: 194rpx;
  height: 62rpx;
  border-radius: 30rpx 30rpx;
   line-height: 55rpx;
  text-align: center;
  margin-top: 30rpx;
}
.inventory .itt{
   display: inline-block;
   width: 70rpx;
   height: 60rpx;
   line-height: 60rpx;
}
.del{
  display: block;
  width: 60rpx;
  height: 60rpx;
    border: 1rpx solid #D2D9EC;
  border-radius: 30rpx 30rpx;
    color: #0D5EFF;
    font-weight:700;
    font-size: 40rpx;
}
.add{
    display: inline-block;
  width: 60rpx;
  height: 60rpx;
      border: 1rpx solid #D2D9EC;
  border-radius: 30rpx 30rpx;
  color: #0D5EFF;
  font-weight:700;
  font-size: 40rpx;

}
/*禁用样式*/
.disable {
  color: #ccc;
}
